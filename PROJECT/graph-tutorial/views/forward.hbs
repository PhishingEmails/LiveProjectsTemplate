<h1>Forward - Report</h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">id</th>
      <th scope="col">Subject</th>
      <th scope="col">Body preview</th>
    </tr>
  </thead>
  <tbody>
    {{#each events}}
      <tr>
        <td>{{this.email.name}}</td>
        <td>{{this.email.subject}}</td>
        <td>{{this.bodyPreview}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>